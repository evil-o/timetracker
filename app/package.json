        "@types/jasminewd2": "2.0.13",
        "@types/node": "22.15.32",
        "@types/uuid": "10.0.0",
        "angular-eslint": "19.7.1",
        "eslint": "9.27.0",
        "jasmine-core": "5.7.1",
        "jasmine-spec-reporter": "7.0.0",
