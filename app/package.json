        "@ngrx/effects": "19.2.1",
        "@ngrx/store": "19.2.1",
        "@ngrx/store-devtools": "19.2.1",
        "bootstrap": "5.3.6",
        "chart.js": "4.4.9",
        "color-convert": "3.1.0",
        "core-js": "3.41.0",
        "immer": "10.1.1",
